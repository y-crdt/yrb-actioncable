    {
      "path": "dist/yrb-actioncable.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "lib0": "^0.2.58",
    "y-protocols": "^1.0.5",
    "yjs": "^13.5.51"
  },
  "devDependencies": {
    "@rails/actioncable": "^7.0.4-3",
    "@size-limit/preset-small-lib": "^8.2.4",
    "@types/actioncable": "^5.2.7",
    "@types/rails__actioncable": "^6.1.6",
    "eslint-config-custom": "*",
    "husky": "^8.0.3",
    "size-limit": "^8.1.0",
    "tsconfig": "*",
    "tsdx": "^0.14.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  }
}
